<div class="container">
  <div class="page-header">
    <h1>Rock &amp; Roll<small> with Ember.JS</small></h1>
  </div>
  <div class="row">
    <div class="container col-md-6">    
      <ul class="list-group">
        {{#each songs as |song|}}
          <li class="list-group-item">{{song.title}} by <em>{{song.band}}</em> 
            <span class="rating pull-right">{{song.rating}}</span>
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
